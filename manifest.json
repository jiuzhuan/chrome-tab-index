{
  "name": "Tab Index",
  "description": "A browser action with a popup dump of all bookmarks, and quickly focus it.",
  "version": "1.1.2",
  "manifest_version": 3,
  "permissions": [
    "bookmarks",
    "tabs",
    "activeTab",
    "storage",
    "notifications"
  ],
  "action": {
    "default_title": "Tab Index",
    "default_icon": "index.png",
    "default_popup": "popup.html"
  }
}
